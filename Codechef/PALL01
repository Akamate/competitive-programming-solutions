//Problem link : https://www.codechef.com/problems/PALL01

#include <iostream>
#include <algorithm>
#include <string>
using namespace std;
int main(int argc, char** argv) {
    int t;
    cin>>t;
    while(t--){
        String n;
        cin>>n;
        n[strlen(n)-1]='\0';
        if(strcmp(n,reverse(n.begin(),n.end()-1))==0)
          count<<"wins"<<endl;
        else cout<<"losses"<<endl;
       }
    
    return 0;
}
